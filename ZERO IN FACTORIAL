import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number:");
        int n=sc.nextInt();
        int c=0
        for(int i=5;n/i>0;i*=5){
            c=c+n/i;
        }
        System.out.print(c);
        }
    }
